<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="700.0" prefWidth="200.0" style="-fx-background-color: gray;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.schedulesoft.controller.SideNavBarController">
   <children>
      <VBox fx:id="logoPane" prefHeight="100.0" style="-fx-background-color: red;" VBox.vgrow="NEVER" />
      <VBox fx:id="profilePane" prefHeight="150.0" style="-fx-background-color: blue;" VBox.vgrow="NEVER" />
      <ListView fx:id="menuListView" prefHeight="150.0" VBox.vgrow="NEVER" />
      <VBox alignment="BOTTOM_LEFT" prefHeight="300.0" style="-fx-background-color: lime;" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="logoutButton" mnemonicParsing="false" text="Logout" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</VBox>
